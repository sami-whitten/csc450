#include <iostream>
#include <string>
using namespace std;

int main() {
	string firstName;
	string lastName;
	string fullName;

	cout << "Enter first name: " << endl;
	cin >> firstName;
	cout << "Enter last name: " << endl;
	cin >> lastName;
	fullName = firstName + " " + lastName;
	cout << "Full name " + fullName;
	return 0;
}
